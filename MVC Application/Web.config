    <add name="DefaultConnection" providerName="System.Data.SqlClient" connectionString="Data Source=(LocalDb)\v11.0;Initial Catalog=aspnet-MVC Application-20150129193403;Integrated Security=SSPI;AttachDBFilename=|DataDirectory|\aspnet-MVC Application-20150129193403.mdf" />
