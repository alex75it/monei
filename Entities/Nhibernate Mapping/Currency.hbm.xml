<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
    assembly="Monei.Entities" namespace="Monei.Entities">
    
    <class name="Currency" table="Currency">
        <id name="Id">
            <generator class="identity" />
        </id>
        <property name="Code" type="AnsiString" length="3" not-null="true" />
        <property name="Name" type="string" length="3" not-null="true" />
        <property name="Symbol" type="string" length="3" not-null="true" />
    </class>
</hibernate-mapping>
