<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
    assembly="Monei.Entities" namespace="Monei.Entities">
    
    <class name="Subcategory" table="Subcategory" lazy="false" >
        <id name="Id">
            <generator class="identity" />
        </id>
        <property name="Name" />
        <property name="Description" />
        
        <property name="CreationDate" />
        <property name="LastChangeDate" />
        <!--<property name="DeletionDate" />-->

        <many-to-one name="Category" column="CategoryId" class="Category" />
                      
    </class>
</hibernate-mapping>
